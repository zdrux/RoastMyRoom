{
  "cli": { "version": ">= 3.13.0", "appVersionSource": "remote" },
  "build": {
    "production": {
      "env": { "EXPO_UNSTABLE_CORE_AUTOLINKING": "1" },
      "android": {
        "buildType": "app-bundle"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": { "buildType": "apk" }
    },
    "preview": {
      "env": { "EXPO_UNSTABLE_CORE_AUTOLINKING": "1" },
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "internal"
      }
    }
  }
}
